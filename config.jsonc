      "(.*) - Obsidian - .* - obsidian": "Ôâä  $1",
      "(.*) - com.mitchellh.ghostty": "Ôíâ  $1",
      "(.*) - Alacritty": "Ôíâ  $1",
      "(.*) - BeeperTexts": "Û∞ªû  $1",
      "(.*) - org.gnome.Nautilus": "Ó™É  $1"
